{
  "name": "fabric-layers-examples",
  "private": true,
  "version": "1.0.0",
  "description": "Examples for fabric-layers packages",
  "workspaces": [
    "fabric-layers-core",
    "fabric-layers-react"
  ],
  "scripts": {
    "start:core": "cd fabric-layers-core && npm start",
